@model App.Models.ProductModel

@{
    var product = ViewBag.product as ProductModel;
}
<p>@ViewData["title"]</p>
<div style="background-color: blue; display: flex; align-items: center; justify-content: center; flex-direction: column;">
    <h1>@product.Name</h1>
    <p>Gia: @product.Price </p>
</div>

@this.Path